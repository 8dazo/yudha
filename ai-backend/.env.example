PORT=3001
OPENROUTER_API_KEY=your_openrouter_api_key_here
# Optional: liquid/lfm-2.5-1.2b-thinking:free (default)
OPENROUTER_MODEL=liquid/lfm-2.5-1.2b-thinking:free
NODE_ENV=development

# 4 accounts: private key + public address (use for treasury, agents, or testing)
ACCOUNT_1_PRIVATE_KEY=
ACCOUNT_1_ADDRESS=
ACCOUNT_2_PRIVATE_KEY=
ACCOUNT_2_ADDRESS=
ACCOUNT_3_PRIVATE_KEY=
ACCOUNT_3_ADDRESS=
ACCOUNT_4_PRIVATE_KEY=
ACCOUNT_4_ADDRESS=

# Optional: generate with node scripts/create-account.js
# BACKEND_ACCOUNT_PRIVATE_KEY=
# BACKEND_ACCOUNT_ADDRESS=

# Market data: optional CoinGecko Pro key for higher rate limits. Otherwise free tier or Binance.
# COINGECKO_API_KEY=
# MARKET_DATA_SOURCE=auto

# Real on-chain sweeps: set these and ENABLE_ONCHAIN_SWEEP=true (after deploying contracts/script/DeployYudha.s.sol)
RPC_URL=https://ethereum-sepolia-rpc.publicnode.com
TREASURY_OWNER_PRIVATE_KEY=
ARC_TREASURY_ADDRESS=
# Arena (play token) contract: deploy with script/DeployArena.s.sol; deducts play tokens on non-HOLD decisions
ARENA_TREASURY_ADDRESS=
# Wallet that holds USDC (for sweepProfit). Can use ACCOUNT_2_ADDRESS. Also player for deductPlay.
AGENT_WALLET=
# Private key for AGENT_WALLET (required for on-chain sweep: backend will approve USDC then sweep). Can use ACCOUNT_2_PRIVATE_KEY.
AGENT_WALLET_PRIVATE_KEY=
# Set to true (or omit) to send real sweep txs; set to false to only simulate
ENABLE_ONCHAIN_SWEEP=true
